	<h1>{pageTitle}</h1>
	<h2>My awesome blog subtitle</h2>
	<Greeting client:load messages={["Hi", "Hello", "Howdy", "Hey there", "Hej", "Hallo", "Hola", "Habari", "Terve"]} />
</BaseLayout>
